{
  "expo": {
    "name": "foresightGraduation",
    "slug": "foresightGraduation",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/mark.png",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#fff"
    },
    "assetBundlePatterns": ["**/*"],
    "extra": {
      "supportsRTL": true
    },
    "plugins": ["expo-localization"],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "CFBundleAllowMixedLocalizations": true
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#fff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "locales": {
      "ar": "./assets/i18n/ar.json",
      "en": "./assets/i18n/en.json"
    }
  }
}
