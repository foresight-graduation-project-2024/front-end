{
  "expo": {
    "name": "foresightGraduation",
    "slug": "foresightGraduation",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/mark.png",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#fff"
    },
    "assetBundlePatterns": ["**/*"],
    "extra": {
      "supportsRTL": true,
      "eas": {
        "projectId": "882ad5fd-75d1-44c2-9884-3b380a52dcd8"
      }
    },
    "plugins": [
      "expo-localization",
      "@react-native-firebase/app",
      [
        "expo-notifications",
        {
          "icon": "./assets/notification_icon.png",
          "color": "#ffffff",
          "sounds": ["./assets/breebles_music.mp3"],
          "mode": "production"
        }
      ]
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "CFBundleAllowMixedLocalizations": true
      }
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "backgroundColor": "#fff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "locales": {
      "ar": "./assets/i18n/ar.json",
      "en": "./assets/i18n/en.json"
    },
    "notification": {
      "icon": "./assets/notification_icon.png"
    }
  }
}
