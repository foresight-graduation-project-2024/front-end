{
  "expo": {
    "name": "Foresight",
    "slug": "foresight",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/mark.png",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#fff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "extra": {
      "supportsRTL": true,
      "eas": {
        "projectId": "8b155b97-0700-45c3-9c3b-e00f2ef2cfc5"
      }
    },
    "plugins": [
      "expo-localization",
      "@react-native-firebase/app",
      [
        "expo-notifications",
        {
          "icon": "./assets/notification_icon.png",
          "sounds": [
            "./assets/breebles_music.mp3"
          ]
        }
      ]
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "CFBundleAllowMixedLocalizations": true,
        "ExpoLocalization_supportsRTL": true
      }
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "backgroundColor": "#fff"
      },
      "package": "com.mostafa7amdy.foresight"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "locales": {
      "ar": "./assets/i18n/ar.json",
      "en": "./assets/i18n/en.json"
    },
    "notification": {
      "icon": "./assets/notification_icon.png"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/8b155b97-0700-45c3-9c3b-e00f2ef2cfc5"
    }
  }
}
